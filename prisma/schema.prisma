// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  size String @default("")
  polivalente Boolean @default(false)
  polivalente2 Boolean @default(false)
  polivalente_refuerzo Boolean @default(false)
  rabia Boolean @default(false)
  age String @default("")
  createAt Int
  location String
  contact String
  email String
  user String
  image String @default("")
}
